{
  "name": "firstpsd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run sass\" \"npm run watch-html\" \"npm run watch-assets\"",
    "server": "live-server ./build",
    "presass": "cp ./src/index.html build/",
    "sass": "node-sass -w ./src/scss/main.scss --include-path ./src/scss/ --include-path ./node_modules/bootstrap/scss/ -o ./build/css/",
    "watch-html": "copy-and-watch --watch src/**/*.{html}  build/",
    "watch-assets": "copy-and-watch --watch assets/**/*.{png}  build/assets/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosueCuberoSanchez/PSD_Practice_1.git"
  },
  "author": "Josué Cubero Sánchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JosueCuberoSanchez/PSD_Practice_1/issues"
  },
  "homepage": "https://github.com/JosueCuberoSanchez/PSD_Practice_1#readme",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "copy-and-watch": "^0.1.2",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.2"
  }
}
